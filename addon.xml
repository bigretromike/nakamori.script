<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="script.module.nakamori" name="Nakamori Script" version="3.3.0~beta1" provider-name="BigRetroMike, da3dsoul">
    <requires>
        <import addon="resource.images.nakamori" version="3.1.0"/>
        <import addon="script.module.nakamori-lib" version="3.2.0"/>
        <import addon="script.module.nakamoriplayer" version="3.1.0"/>
        <import addon="script.module.pil" version="1.1.7"/>
        <import addon="script.module.pydevd-pycharm" optional="true"/>
        <import addon="xbmc.python" version="2.25.0"/>
        <import addon="script.module.routing" version="0.2.3"/>
